<?xml version="1.0" encoding="UTF-8"?>
<archetype-descriptor xsi:schemaLocation="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0 http://maven.apache.org/xsd/archetype-descriptor-1.0.0.xsd" name="Root"
    xmlns="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

  <modules>
    <module id="Project-dto" dir="Project-dto" name="Project-dto">
      <fileSets>
        <fileSet filtered="true" encoding="UTF-8">
          <directory>src/main/java</directory>
          <includes>
            <include>**/*.java</include>
          </includes>
        </fileSet>
        <fileSet encoding="UTF-8">
          <directory>src/main/java</directory>
          <includes>
            <include>**/*.config</include>
          </includes>
        </fileSet>
        <fileSet encoding="UTF-8">
          <directory>src/main/resources</directory>
          <includes>
            <include>**/*.MF</include>
          </includes>
        </fileSet>
      </fileSets>
    </module>
    <module id="Project-ear" dir="Project-ear" name="Project-ear">
      <fileSets>
        <fileSet encoding="UTF-8">
          <directory>src/main/application</directory>
          <includes>
            <include>**/*.MF</include>
          </includes>
        </fileSet>
      </fileSets>
    </module>
    <module id="Project-ejb" dir="Project-ejb" name="Project-ejb">
      <fileSets>
        <fileSet filtered="true" encoding="UTF-8">
          <directory>src/main/java</directory>
          <includes>
            <include>**/*.java</include>
          </includes>
        </fileSet>
        <fileSet filtered="true" encoding="UTF-8">
          <directory>src/main/resources</directory>
          <includes>
            <include>**/*.xml</include>
          </includes>
        </fileSet>
        <fileSet encoding="UTF-8">
          <directory>src/main/resources</directory>
          <includes>
            <include>**/*.MF</include>
          </includes>
        </fileSet>
      </fileSets>
    </module>
  </modules>
    <requiredProperties>
  	<requiredProperty key="restUrl">
  		<defaultValue>rst</defaultValue>
  	</requiredProperty>
  	<requiredProperty key="websocketUrl">
  		<defaultValue>ws</defaultValue>
  	</requiredProperty>
  </requiredProperties>
</archetype-descriptor>
